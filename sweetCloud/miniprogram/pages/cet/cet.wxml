<form bindsubmit="bindFormSubmit">

<view class='weui-cell weui-cell_input'>
<input placeholder='输入你的姓名' bindinput='nameInput' class="weui-input input-before" name="name"></input>

</view>
<view class='weui-cell weui-cell_input'>
  <input placeholder='输入你的15位准考证号' bindinput='noInput' maxlength='15' class="weui-input" name="noValue"></input>
</view>


<button form-type="submit" >预存准考证号</button>
</form>

<view class='sw-card'>
  <view class='weui-cell'>
教育网四六级查询
    <text bindtap='copyNeea'>http://cet.neea.edu.cn/cet</text>
</view>
</view>

<view class='sw-card'>
  <view class='weui-cell'>
    学信网四六级查询
  <text bindtap='copyChsi'>https://www.chsi.com.cn/cet/</text>
  </view>
</view>

